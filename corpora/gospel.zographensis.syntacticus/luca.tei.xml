        <lg id="1_1" n="1">
          <cl id="1_1_1"><w lemma="понѥже">Пон҄еже</w> <w lemma="оубо">оубо</w>. <w lemma="мъногъ">мнози</w> <w lemma="начѧти">начѧшѧ</w>. <w lemma="чинити">чинити</w> <w lemma="повѣсть">повѣсть</w>. <w lemma="о">о</w> <w lemma="извѣстовати">ꙇзвѣстънъꙇхъ</w> <w lemma="въ">въ</w> <w lemma="мꙑ">насъ</w> <w lemma="вещь">вештехъ</w>. <w lemma="ꙗкоже">ѣкоже</w> <w lemma="прѣдати">прѣдашѧ</w> <w lemma="мꙑ">намъ</w>. <w lemma="бꙑти">бꙑвъшеі</w> <w lemma="искони">ꙇскони</w> <w lemma="самовидьць">самовидьци</w>. <w lemma="и">ꙇ</w> <w lemma="слоуга">слоугꙑ</w> <w lemma="слово">словеси</w>. <w lemma="изволити">ꙇзволи</w> <w lemma="себе">сѧ</w> <w lemma="и">ꙇ</w> <w lemma="азъ">мьнѣ</w> <w lemma="ходити">хождьшю</w>. <w lemma="испрьва">ꙇспръва</w> <w lemma="по">по</w> <w lemma="вьсь">всѣхъ</w>. <w lemma="въ">въ</w> <w lemma="истина">ꙇстинѫ</w> <w lemma="по">по</w> <w lemma="рѧдъ">рѧдоу</w>. <w lemma="пьсати">псати</w> <w lemma="тꙑ">тебѣ</w>. <w lemma="славьнъ">славънъꙇ</w> <w lemma="ѳеофилъ">тⸯеофиле</w>. <w lemma="да">да</w> <w lemma="разоумѣти">разоумѣеши</w>. <w lemma="о">о</w> <w lemma="иже">н҄ихъже</w> <w lemma="наоучити">наоучилъ</w> <w lemma="себе">сѧ</w> <w lemma="бꙑти">еси</w> <w lemma="слово">словесехъ</w>. <w lemma="оутврьждениѥ">оутвръжденье</w>. </cl>
